{
    "name": "realtime-notes",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "date && tsx watch --clear-screen=false --env-file=.env.development src/index.ts",
        "dev:staging": "tsx --watch --clear-screen=false  --env-file .env.staging src/index.ts",
        "dev:prod": "tsx --watch ---clear-screen=false  -env-file .env.production src/index.ts",
        "start": "node dist/index.js",
        "build": "eslint . && tsc",
        "debug": "tsx watch --clear-screen=false --inspect-brk src/index.ts",
        "type-check": "tsc --noEmit",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [],
    "author": "siddhant deval",
    "license": "ISC",
    "type": "commonjs",
    "dependencies": {
        "express": "^5.1.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.37.0",
        "@tsconfig/node22": "^22.0.2",
        "@types/express": "^5.0.3",
        "@types/node": "^24.7.2",
        "eslint": "^9.37.0",
        "prettier": "^3.6.2",
        "tsx": "^4.20.6",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.1"
    }
}